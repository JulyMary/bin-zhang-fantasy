    <!-- Resource Dictionary -->
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Brushes.xaml"/>
    </ResourceDictionary.MergedDictionaries>
  
    <!-- DownRepeatButtonTempalte -->
    <ControlTemplate x:Key="OrangeScrollBarButtonDownTemplate" TargetType="{x:Type RepeatButton}">
        <Grid FocusManager.IsFocusScope="True">
            <Border>
                <ContentPresenter Style="{StaticResource ContentPresenterStyle}" >
                    <ContentPresenter.Content>
                        <Border Name="DownButton" Width="10" Height="10" Background="{StaticResource ScrollDownButtonBrush.Orange}"/>
                    </ContentPresenter.Content>
                </ContentPresenter>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="DownButton" Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="DownButton" Value="{StaticResource ScrollDownButtonBrushHover.Orange}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="DownButton" Value="{StaticResource ScrollDownButtonPressedBrush.Orange}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    
    <!-- UpRepeatButtonTemplate -->
    <ControlTemplate x:Key="OrangeScrollBarButtonUpTemplate" TargetType="{x:Type RepeatButton}">
        <Grid FocusManager.IsFocusScope="True">
           <Border>
                <ContentPresenter Style="{StaticResource ContentPresenterStyle}" >
                    <ContentPresenter.Content>
                        <Border Name="DownButton" Width="10" Height="10" Background="{StaticResource ScrollUpButtonBrush.Orange}"/>
                    </ContentPresenter.Content>
                </ContentPresenter>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="DownButton" Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="DownButton" Value="{StaticResource ScrollUpButtonBrushHover.Orange}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="DownButton" Value="{StaticResource ScrollUpButtonPressedBrush.Orange}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>   

