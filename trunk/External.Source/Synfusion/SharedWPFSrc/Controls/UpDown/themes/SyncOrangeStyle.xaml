            <DrawingGroup ClipGeometry="F1 M 0,0L 103,0L 103,60L 0,60L 0,0">
                <DrawingGroup.Children>
                    <GeometryDrawing Brush="#FFB54A25" Geometry="F1 M -353.467,1.18158C -359.495,1.18158 -364.373,6.06158 -364.373,12.0869L -364.373,49.5776C -364.373,55.6069 -359.495,60.4883 -353.467,60.4883L 0.815186,60.4883L 37.8072,1.18158L -353.467,1.18158 Z " />
                    <DrawingGroup ClipGeometry="F1 M -539,-498L 277,-498L 277,558L -539,558L -539,-498 Z ">
                        <DrawingGroup.Children>
                            <GeometryDrawing Brush="#FF6E2612" Geometry="F1 M 91.612,60.4883C 97.6334,60.4883 102.523,55.6069 102.523,49.5776L 102.523,12.0869C 102.523,6.06158 97.6334,1.18158 91.612,1.18158L 52.1586,1.18158L 15.1613,60.4883L 91.612,60.4883 Z " />
                        </DrawingGroup.Children>
