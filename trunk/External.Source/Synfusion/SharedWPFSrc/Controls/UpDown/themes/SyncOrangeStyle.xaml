                    <GeometryDrawing Brush="#FFEEE413" Geometry="F1 M 599.065,69.3724L 617.457,69.3724L 608.264,51.5564L 599.065,69.3724 Z ">
                        <GeometryDrawing.Pen>
                            <Pen Thickness="1.33333" LineJoin="Round" Brush="#FF6E2612"/>
