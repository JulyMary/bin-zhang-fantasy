                    </GeometryDrawing>
                </DrawingGroup.Children>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <DrawingBrush x:Key="ScrollUpButtonBrush.Orange">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <DrawingGroup.Children>
                    <GeometryDrawing Brush="#FF6E2612" Geometry="F1 M 599.51,68.9284L 617.902,68.9284L 608.709,51.1124L 599.51,68.9284 Z "/>
                </DrawingGroup.Children>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <DrawingBrush x:Key="ScrollUpButtonPressedBrush.Orange">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <DrawingGroup.Children>
                    <GeometryDrawing Brush="#FF5F1A11" Geometry="F1 M 599.065,69.3724L 617.457,69.3724L 608.264,51.5564L 599.065,69.3724 Z ">
                        <GeometryDrawing.Pen>
                            <Pen Thickness="1.33333" LineJoin="Round" Brush="#FFEEE413"/>
                        </GeometryDrawing.Pen>
