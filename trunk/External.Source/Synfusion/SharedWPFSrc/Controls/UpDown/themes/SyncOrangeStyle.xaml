    <DrawingBrush x:Key="ScrollDownButtonPressedBrush.Orange">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <DrawingGroup.Children>
                    <GeometryDrawing Brush="#FF5F1A11" Geometry="F1 M 599.065,78.2227L 617.457,78.2227L 608.264,96.0387L 599.065,78.2227 Z ">
                        <GeometryDrawing.Pen>
                            <Pen Thickness="1.33333" LineJoin="Round" Brush="#FFEEE413"/>
                        </GeometryDrawing.Pen>
                    </GeometryDrawing>
                </DrawingGroup.Children>
            </DrawingGroup>
        </DrawingBrush.Drawing>
