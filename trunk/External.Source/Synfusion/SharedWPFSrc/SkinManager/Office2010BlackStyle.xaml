    <Style x:Key="Office2010BlackFlowDocumentPageViewerStyle" TargetType="{x:Type FlowDocumentPageViewer}">
        <Setter Property="Foreground" Value="{StaticResource Office2010BlackFlowDocumentForeground}"/>
    </Style>
    <Style BasedOn="{StaticResource Office2010BlackFlowDocumentPageViewerStyle}" TargetType="{x:Type FlowDocumentPageViewer}"/>

    <Style x:Key="Office2010BlackFlowDocumentReaderStyle" TargetType="{x:Type FlowDocumentReader}">
        <Setter Property="Foreground" Value="{StaticResource Office2010BlackFlowDocumentForeground}"/>
    </Style>
    <Style BasedOn="{StaticResource Office2010BlackFlowDocumentReaderStyle}" TargetType="{x:Type FlowDocumentReader}"/>

    <Style x:Key="{x:Type Italic}"
