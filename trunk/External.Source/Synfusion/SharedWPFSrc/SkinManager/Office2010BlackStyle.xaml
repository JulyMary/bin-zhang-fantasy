                            <Setter Property="Fill" TargetName="B1" Value="{StaticResource Office2010BlackSubMenuItemMouseOverBorderBrush}"/>
                            <Setter Property="Fill" TargetName="L1" Value="{StaticResource Office2010BlackSubMenuItemMouseOverBorderBrush}"/>
                            <Setter Property="Fill" TargetName="T1" Value="{StaticResource Office2010BlackSubMenuItemMouseOverBorderBrush}"/>
                            <Setter Property="Margin" TargetName="ContentContainer" Value="3,3,1,1"/>
                            <Setter Property="Background" TargetName="scrollButton" Value="{StaticResource Office2010BlackSubMenuItemMouseOverBackground}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
