            </Trigger>
            <Trigger Property="IsHighlighted" Value="true">
                <Setter Property="Fill" TargetName="Bg" Value="{StaticResource Office2010BlackSubMenuMouseOverBackground}"/>
                <Setter Property="Stroke" TargetName="Bg" Value="{StaticResource Office2010BlackSubMenuMouseOverBorderBrush}"/>
