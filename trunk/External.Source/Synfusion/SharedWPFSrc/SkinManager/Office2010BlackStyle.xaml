    <!-- Button Style-->
    
    <Style x:Key="Office2010BlackButtonFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle Margin="2" SnapsToDevicePixels="true" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="Office2010BlackButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="FocusVisualStyle" Value="{StaticResource Office2010BlackButtonFocusVisual}"/>
        <Setter Property="Background" Value="{StaticResource Office2010BlackButtonBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Office2010BlackButtonBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Foreground" Value="{StaticResource Office2010BlackButtonForeground}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="Chrome" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" SnapsToDevicePixels="true" BorderThickness="{TemplateBinding BorderThickness}">
                        <Border x:Name="innerBorder" Background="Transparent" BorderBrush="Transparent" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="True">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlackButtonMouseOverBorderBrush}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="#FFF8F4E3"/>
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlackButtonMouseOverBackground}"/>
                            <Setter Property="Foreground" Value="Black"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlackButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="{StaticResource Office2010BlackButtonInnerBorderPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlackButtonPressedBorderBrush}"/>
                            <Setter Property="Foreground" Value="Black"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="#ADADAD"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="Button" BasedOn="{StaticResource Office2010BlackButtonStyle}"/>
    
    <!-- Toggle Button Brushes-->

    <Style x:Key="Office2010BlackToggleButtonStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="FocusVisualStyle" Value="{StaticResource Office2010BlackButtonFocusVisual}"/>
        <Setter Property="Background" Value="{StaticResource Office2010BlackToggleButtonBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Office2010BlackToggleButtonBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Foreground" Value="{StaticResource Office2010BlackToggleButtonForeground}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Border x:Name="Chrome" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" SnapsToDevicePixels="true" BorderThickness="1">
                        <Border x:Name="innerBorder" Background="Transparent" BorderBrush="Transparent" BorderThickness="1">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonMouseOverBorderBrush}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="#FFF8F4E3"/>
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonMouseOverBackground}"/>
                            <Setter Property="Foreground" Value="Black"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="{StaticResource Office2010BlackInnerBorderToggleButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonPressedBorderBrush}"/>
                            <Setter Property="Foreground" Value="Black"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="{StaticResource Office2010BlackInnerBorderToggleButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlackToggleButtonPressedBorderBrush}"/>
                            <Setter Property="Foreground" Value="Black"/>
                        </Trigger>

                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="#ADADAD"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

