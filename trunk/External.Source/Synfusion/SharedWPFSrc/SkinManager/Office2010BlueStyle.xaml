                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlueButtonMouseOverBackground}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="Chrome" Value="{StaticResource Office2010BlueButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="innerBorder" Value="{StaticResource Office2010BlueButtonInnerBorderPressedBorderBrush}"/>
                            <Setter Property="BorderBrush" TargetName="Chrome" Value="{StaticResource Office2010BlueButtonPressedBorderBrush}"/>
