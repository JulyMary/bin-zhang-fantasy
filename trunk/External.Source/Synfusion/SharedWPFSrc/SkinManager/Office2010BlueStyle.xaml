                                <Ellipse x:Name="ellipse3" Fill="{StaticResource Office2010BlueRadioButtonInnerEllipseBackground}" Stroke="{StaticResource Office2010BlueRadioButtonInnerEllipseBorderBrush}" StrokeThickness="1" Canvas.Left="3" Canvas.Top="3" Width="7" Height="7" SnapsToDevicePixels="True">
                                </Ellipse>
                                <Ellipse x:Name="ellipse" Canvas.Left="4" Canvas.Top="4" Width="5" Height="5" SnapsToDevicePixels="True">
                                </Ellipse>
                            </Canvas>
                        </BulletDecorator.Bullet>
                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </BulletDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HasContent" Value="true">
                            <Setter Property="FocusVisualStyle" Value="{StaticResource Office2010BlueRadioBoxFocusVisual}"/>
                            <Setter Property="Padding" Value="4,0,0,0"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Fill" TargetName="ellipse" Value="{StaticResource Office2010BlueRadioButtonPathBackground}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="ellipse1" Property="Stroke" Value="{StaticResource Office2010BlueRadioButtonOuterEllipsePressedBorderBrush}"/>
                            <Setter TargetName="ellipse3" Property="Fill" Value="{StaticResource Office2010BlueRadioButtonInnerEllipsePressedBackground}" />
                            <Setter TargetName="ellipse3" Property="Stroke" Value="{StaticResource Office2010BlueRadioButtonInnerEllipsePressedBorderBrush}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="True"/>
                                <Condition Property="IsMouseOver" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="ellipse1" Property="Fill" Value="{StaticResource Office2010BlueRadioButtonOuterBorderMouseOverBackground}" />
                            <Setter TargetName="ellipse1" Property="Stroke" Value="{StaticResource Office2010BlueRadioButtonOuterBorderMouseOverBorderBrush}" />
                            <Setter TargetName="ellipse3" Property="Fill" Value="{StaticResource Office2010BlueRadioButtonInnerBorderMouseOverBackground}" />
                            <Setter TargetName="ellipse3" Property="Stroke" Value="{StaticResource Office2010BlueRadioButtonInnerBorderMouseOverBorderBrush}" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
