    <Style TargetType="ToggleButton" x:Key="TileViewMinMaxButtonStyle">
        <Setter Property="Background" Value="{StaticResource TileViewMinMaxButtonBackgroundBrushKey}"/>
        <Setter Property="Foreground" Value="{StaticResource TextBrushKey}"/>
