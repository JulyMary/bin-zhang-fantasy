        <Button x:Name="ignore" Grid.Row="1" Grid.Column="1" Margin="3,0,3,5" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Ignore Once" Click="ignoreOnce" />
        <Button x:Name="ignoreAllBtn" Grid.Row="2" Grid.Column="1" Margin="3" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Ignore All" Click="ignoreAll" />
        <Button x:Name="addToDic"  Grid.Row="3" Grid.Column="1" Margin="3" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Add to Dictionary" Click="addToDictionary" />
        <Button x:Name="replaceBtn" Grid.Row="5" Grid.Column="1" Margin="3,0,3,5" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Change" Click="replace" />
        <Button x:Name="replaceAllBtn" Grid.Row="6" Grid.Column="1" Margin="3" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Change All" Click="replaceAll" />
        <Button x:Name="closeBtn" Grid.Row="7" Grid.Column="1" Margin="3" Height="30" Style="{StaticResource Office2010SilverButtonStyle}" Content="Close" Click="closeBtn_Click" />
        
    </Grid>
