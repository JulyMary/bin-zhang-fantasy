        <GradientStop Color="#FFFFF3CF" Offset="0.492"/>
        <GradientStop Color="#FFFFECB5" Offset="0.499"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ItemMouseOverBorderBrush" Color="#FFE5C365"/>

    <SolidColorBrush x:Key="itemCheckedBorderBrush" Color="#FFE5C365"/>
    <SolidColorBrush x:Key="itemCheckedBackgroundBrush" Color="#FFFEE7A5"/>
