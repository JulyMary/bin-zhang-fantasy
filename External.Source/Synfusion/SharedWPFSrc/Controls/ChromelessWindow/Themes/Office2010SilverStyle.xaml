            <Trigger Property="IsMouseOver" Value="true">
                <Setter TargetName="closeborder" Property="Background" Value="{StaticResource Office2010SilverMouseOverBackground}"/>
                <Setter TargetName="closeborder" Property="BorderBrush" Value="{StaticResource Office2010SilverMouseOverBorderBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="true">
                <Setter TargetName="closeborder" Property="Background" Value="{StaticResource Office2010SilverPressedBackground}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="Office2010SilverTitleBarTemplate" TargetType="{x:Type local:TitleBar}">
        <Border Name="border"  Height="30"  CornerRadius="5,5,0,0" Background="{StaticResource Office2010SilverTitleBarBackground}" >
            <Border BorderBrush="{StaticResource Office2010SilverTitleBarBorderBrush}" Background="Transparent" BorderThickness="0,0,0,1" Width="Auto" CornerRadius="5,5,0,0">
                <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Bottom"  Margin="0,0,0,0"/>
            </Border>
        </Border>
    </ControlTemplate>

    <Style x:Key="Office2010SilverChromelessWindowStyle" TargetType="{x:Type local:ChromelessWindow}" >
        <!--<Setter Property="AllowsTransparency" Value="true" />-->
        <Setter Property="ResizeGripStyle" Value="{StaticResource ResizeStyle}"/>
        <Setter Property="WindowStyle" Value="None" />
        <Setter Property="ResizeBorderThickness" Value="2" />
        <Setter Property="SnapsToDevicePixels" Value="False"/>
        <Setter Property="Icon" Value="..\Resources\app.png"/>
        <Setter Property="CornerRadius" Value="5"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="ResizeBorderBrush" Value="{StaticResource OrangeRedBorderBrush}" />
        <Setter Property="MinWidth" Value="300"/>
        <Setter Property="MinHeight" Value="120"/>
        <!--<Setter Property="Focusable" Value="False"/>-->

        <Setter Property="TitleBarBackground" Value="{StaticResource Office2010SilverTitleBarBackground}"/>
        <Setter Property="Background" Value="{StaticResource Office2010SilverBackground}"/>
        <Setter Property="ResizeBorderBrush" Value="{StaticResource Office2010SilverBorderBrush}"/>
        <Setter Property="MinimizeButtonTemplate" Value="{StaticResource MinimizeSilver}"/>
        <Setter Property="MaximizeButtonTemplate" Value="{StaticResource MaximizeSilver}"/>
        <Setter Property="RestoreButtonTemplate" Value="{StaticResource RestoreSilver}"/>
        <Setter Property="CloseButtonTemplate" Value="{StaticResource CloseSilver}"/>
        <Setter Property="TitleBarTemplate" Value="{StaticResource Office2010SilverTitleBarTemplate}"/>
        <Setter Property="Foreground" Value="{StaticResource Office2010SilverForeground}"/>
        <Setter Property="Template" Value="{StaticResource SkinTemp}"/>
    </Style>

    <Style BasedOn="{StaticResource Office2010SilverChromelessWindowStyle}" TargetType="{x:Type local:ChromelessWindow}" />

</ResourceDictionary>