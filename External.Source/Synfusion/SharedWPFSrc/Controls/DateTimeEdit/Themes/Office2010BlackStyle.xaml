        <GradientStop Color="#FFF6C867" Offset="0.043"/>
        <GradientStop Color="#FFFBD678" Offset="0.049"/>
        <GradientStop Color="#FFFFE48A" Offset="0.21"/>
        <GradientStop Color="#FFFEE287" Offset="0.962"/>
        <GradientStop Color="#FFFAD77A" Offset="0.968"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Office2010BlackRepeatButtonPressedBorderBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFC2762B" Offset="0"/>
        <GradientStop Color="#FFC29E47" Offset="1"/>
