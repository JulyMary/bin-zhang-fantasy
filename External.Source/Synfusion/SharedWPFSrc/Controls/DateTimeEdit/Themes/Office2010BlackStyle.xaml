        <GradientStop Color="#FFECD37B" Offset="0.351"/>
        <GradientStop Color="#FFEED885" Offset="0.594"/>
        <GradientStop Color="#FFF3E7B0" Offset="0.723"/>
        <GradientStop Color="#FFF6F3D9" Offset="0.969"/>
