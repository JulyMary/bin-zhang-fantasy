                         StartPoint="-100,0"
                         EndPoint="0,0">
            <LinearGradientBrush.GradientStops>
                <GradientStop Color="#00000000"
                          Offset="0"/>
                <GradientStop Color="#FF000000"
                          Offset="0.4"/>
