    <Style BasedOn="{StaticResource Office2007BlackComboBoxStyle}" TargetType="{x:Type ComboBox}"/>

    <Style x:Key="Office2007BlackComboBoxItemStyle"
           TargetType="{x:Type ComboBoxItem}">
        <Setter Property="HorizontalContentAlignment"
                Value="{Binding Path=HorizontalContentAlignment,RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
        <Setter Property="VerticalContentAlignment"
                Value="{Binding Path=VerticalContentAlignment,RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
        <Setter Property="Padding"
                Value="3,0,3,0"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Background" Value="{StaticResource ComboBoxItemBackground.Blue}"/>
        <Setter Property="BorderBrush" Value="{StaticResource ComboBoxItemBorderBrush.Blue}"/>
        <Setter Property="Foreground" Value="{StaticResource  ForegroundBrush.Blue}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ComboBoxItem}">
                    <Border Name="Bd" CornerRadius="4" Margin="2"
                            Background="{TemplateBinding Background}"
                            BorderBrush="Transparent" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Padding="{TemplateBinding Padding}"
                            SnapsToDevicePixels="true">
                        <ContentPresenter Margin="2" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsHighlighted" Value="true">
                            <Setter Property="BorderBrush" TargetName="Bd" Value="{StaticResource BorderBrush.Blue}"/>
                            <Setter Property="Background" TargetName="Bd" Value="{StaticResource ButtonMouseOverBrush.Blue}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Opacity" Value="0.5"/>
