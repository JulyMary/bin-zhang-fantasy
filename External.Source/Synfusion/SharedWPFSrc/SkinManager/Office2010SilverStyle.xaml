        <Setter Property="Height" Value="16"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Border Background="Transparent" Height="16" Padding="5,5,5,5" Width="16" SnapsToDevicePixels="True">
                        <Path x:Name="ExpandPath" Data="{StaticResource TreeArrow}" Fill="{StaticResource Office2010SilverTreeViewExpanderBackground}" Stroke="{StaticResource Office2010SilverTreeViewExpanderBorderBrush}" SnapsToDevicePixels="True">
