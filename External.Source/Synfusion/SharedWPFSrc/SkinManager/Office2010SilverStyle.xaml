<!-- ToolTip Styles-->
    <Style x:Key="Office2010SilverToolTipStyle"  TargetType="{x:Type ToolTip}">
        <Setter Property="BorderThickness"  Value="1"/>
        <Setter Property="BorderBrush" Value="{StaticResource Office2010SilverToolTipBorderBrush}"/>
        <Setter Property="Background" Value="{StaticResource Office2010SilverToolTipBackground}"/> 
        <Setter Property="Foreground" Value="{StaticResource Office2010SilverToolTipForeground}"/>
        <Setter Property="FontFamily"
                Value="{DynamicResource {x:Static SystemFonts.StatusFontFamilyKey}}"/>
        <Setter Property="FontSize"
                Value="{DynamicResource {x:Static SystemFonts.StatusFontSizeKey}}"/>
