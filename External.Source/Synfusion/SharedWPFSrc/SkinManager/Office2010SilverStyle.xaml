        <Setter Property="BorderBrush" Value="{StaticResource Office2010SilverListViewBorderBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource Office2010SilverListViewForeground}"/>
        <Setter Property="BorderThickness"  Value="1"/>
