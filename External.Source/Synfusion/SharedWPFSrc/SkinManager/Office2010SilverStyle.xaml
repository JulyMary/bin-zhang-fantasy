        <Setter Property="TextWrapping" Value="NoWrap"/>
        <Setter Property="TextTrimming" Value="None"/>
        <Setter Property="Foreground" Value="{StaticResource Office2010SilverTextBlockForeground}"/>
    </Style>
    <Style TargetType="TextBlock" BasedOn="{StaticResource Office2010SilverTextBlockStyle}"/>
    
<!-- RichTextBox Styles -->
    <Style x:Key="Office2010SilverRichTextBoxStyle" TargetType="{x:Type RichTextBox}">
        <Style.Resources>
            <Style x:Key="{x:Type FlowDocument}"  TargetType="{x:Type FlowDocument}">
                <Setter Property="OverridesDefaultStyle" Value="true"/>
