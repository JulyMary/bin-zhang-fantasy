                                <Condition Property="IsSelectionActive" Value="false"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" TargetName="Bd" Value="{StaticResource Office2010SilverTreeViewItemSelectedBackground}"/>
                            <Setter Property="Foreground" Value="{StaticResource Office2010SilverTreeViewItemSelectedForeground}"/>
