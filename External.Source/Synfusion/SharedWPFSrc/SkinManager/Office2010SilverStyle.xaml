        </Style.Resources>
        <Setter Property="MinWidth" Value="10"/>
    </Style>

    <Style BasedOn="{StaticResource Office2010SilverRichTextBoxStyle}" TargetType="{x:Type RichTextBox}"/>
    
<!-- HyperLink Style -->    

    <Style x:Key="Office2010SilverHyperLinkStyle" TargetType="{x:Type Hyperlink}">
        <Setter Property="Foreground"   Value="Blue"/>
        <Setter Property="TextDecorations" Value="Underline"/>
        <Style.Triggers>
