                                </Setter.Value>
                            </Setter>
                            <Setter Property="Fill" TargetName="ExpandPath" Value="{StaticResource Office2010SilverTreeViewExpanderCheckedBackground}"/>
                            <Setter Property="Stroke" TargetName="ExpandPath" Value="{StaticResource Office2010SilverTreeViewExpanderCheckedBorderBrush}"/>
