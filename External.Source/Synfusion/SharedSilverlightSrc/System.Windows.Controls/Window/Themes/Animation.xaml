        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationY)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="90"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="170"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="220"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="300"/>
