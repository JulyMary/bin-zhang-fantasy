<Window x:Class="Fantasy.Studio.BusinessEngine.BusinessClassPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:be="urn:schema-fantasy:business-engine"
        xmlns:sbe="clr-namespace:Fantasy.Studio.BusinessEngine" 
        Title="Select Data Type" Height="600" Width="800" ResizeMode="CanMinimize" xmlns:f="urn:schema-fantasy:xaml">
    <Window.Resources>
        <sbe:PackageChildrenConverter x:Key="PackageChildren" />
    </Window.Resources>
    <Grid>
       
        <f:ExtendableTreeView DataContext="{Binding TreeViewModel}"  Height="508" HorizontalAlignment="Left" Margin="12,12,0,0" Name="extendableTreeView" VerticalAlignment="Top" Width="754" SelectedItemChanged="extendableTreeView_SelectedItemChanged" MouseDoubleClick="extendableTreeView_MouseDoubleClick" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="610,526,0,0" Name="OkButton" VerticalAlignment="Top" Width="75" IsDefault="True" Click="OkButton_Click" IsEnabled="{Binding IsOKEnabled}"   />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="691,526,0,0" Name="CancelButton" VerticalAlignment="Top" Width="75" IsCancel="True" Click="CancelButton_Click" />
    </Grid>
</Window>
