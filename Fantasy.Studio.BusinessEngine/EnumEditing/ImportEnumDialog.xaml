<Window x:Class="Fantasy.Studio.BusinessEngine.EnumEditing.ImportEnumDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ImportEnumDialog" Height="600" Width="800" xmlns:f="urn:schema-fantasy:xaml">
    <Grid>
        <f:ExtendableTreeView Margin="12,12,0,41" Name="enumsTreeView" HorizontalAlignment="Left" Width="290" />
        <Button Content="OK" HorizontalAlignment="Right" Margin="0,0,93,12" Name="OkButton" Width="75" IsDefault="True" Click="OkButton_Click" IsEnabled="{Binding IsOKEnabled}" Height="23" VerticalAlignment="Bottom" />
        <Button Content="Cancel" Height="23" Margin="0,0,12,12" Name="CancelButton" VerticalAlignment="Bottom" IsCancel="True" Click="CancelButton_Click" HorizontalAlignment="Right" Width="75" />
    </Grid>
</Window>
